{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "lambda-nodejs14.x:HelloWorldFunction (nodejs14.x)",
      "invokeTarget": {
        "target": "template",
        "templatePath": "${workspaceFolder}/services/lambda-nodejs14.x/template.yaml",
        "logicalId": "HelloWorldFunction"
      },
      "lambda": {
        "payload": {},
        "environmentVariables": {},
        "runtime": "nodejs14.x"
      }
    },
    {
      "type": "aws-sam",
      "request": "direct-invoke",
      "name": "API lambda-nodejs14.x:HelloWorldFunction (nodejs14.x)",
      "invokeTarget": {
        "target": "api",
        "templatePath": "${workspaceFolder}/services/lambda-nodejs14.x/template.yaml",
        "logicalId": "HelloWorldFunction"
      },
      "api": {
        "path": "/hello",
        "httpMethod": "get",
        "payload": {
          "json": {}
        }
      },
      "lambda": {
        "runtime": "nodejs14.x"
      }
    },
    {
      "name": "Attach to SAM CLI",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}/services",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false
    }
  ]
}
